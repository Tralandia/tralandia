<script type="text/javascript" n:syntax="double">
    $(function() {
        $.nette.ext({
            error: function(request) {
                window.open(request.getResponseHeader('X-Nette-Error-Log'));
            }
        });
    });
</script>
{control head:end}
{includeblock APP_DIR.'/BaseModule/forms/@formControls.latte'}

<body>
	<div class="container baseRadius" id="container">

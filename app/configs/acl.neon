parameters:
	acl:
		roles:
			- guest
			- [member, guest]
			- [admin, member]

		resources:
			# presenters
			- Admin:Location
			- Admin:Language
			- Admin:Phrase

			# entities
			- Entity\Dictionary\Language
			- Entity\Location\Location

		rules:
			allow:
				- [guest, [Admin:Location, Admin:Language, Admin:Phrase], [list, show, edit]]
				- [guest, Entity\Dictionary\Language, edit, [Security\MyAssertion, test]]
				- [guest, Entity\Location\Location, edit]
			deny:
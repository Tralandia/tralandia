<div class="span12">
	<div class="innerPadding baseRadius baseBorder marginBottom overflow">
		<div id="content" class="nopadding">
			

<div class="row-fluid objectDetailContent">
	<div class="span4 marginBottom marginTop marginLeft leftSidebarObjectDetail">
		{var $ii = 1}
		{foreach $rental->getImages(3) as $photo}
			<img alt="{_$rental->name} {$ii}" src="{$basePath}{$photo|rentalImageSrc}" class="marginBottom" />
			{var $ii = $ii + 1}
		{/foreach}
		
		 <img class="mapsImg" src="https://maps.googleapis.com/maps/api/staticmap?center={$rental->address->getGps()->latitude},{$rental->address->getGps()->longitude}&markers=color:blue%7C{$rental->address->getGps()->latitude},{$rental->address->getGps()->longitude}&zoom=10&size=288x170&sensor=false" /> 
		
	</div>



	<div class="span5">

		<h1 class="addToViewList" id="{$rental->id}">{_$rental->name}</h1>

		<p class="teaser red bold nopadding marginBottom">			
			{_$rental->teaser|firstUpper}
		</p>
		<p class="marginBottom nopadding">
		<a class="capitalize brown"  href="{link Rental:list location => $rental->address->locality , rentalType => $rental->type }">{_$rental->type->name|firstUpper} {_$rental->address->locality->name , null , [case=>locative]}</a>
		</p>

		{foreach $rental->address->getLocations() as $location}

			<a href="{link Rental:list location => $location}">{_$location->name}</a>{if !$iterator->last}, {/if}
			
		{/foreach}




		<div class="metainfobox double"> 
			<div class="separator first"><i class="entypo-info2"></i>{$rental->price} #Eur <small>/ osoba / noc</small> </div>
			<div class="separator second"><i class="entypo-users"></i><span class="capitalize">{_o1008}.</span> <small>{$rental->maxCapacity} {_o925,null,[count=>$rental->maxCapacity]}</small></div>
			<div class="clearfix"></div>
		</div>
		
		<div class="clearfix marginBottom"></div>

		{if isset($rental->checkIn)}
		<div class="metainfobox marginBottom">
			<div class="separator first"><i class="entypo-clock"></i> #Check-in: <small>{$rental->checkIn} </small></div> 
			<div class="separator second"><i class="entypo-clock"></i> #Check-out: <small>{$rental->checkOut}</small></div>
			<div class="clearfix"></div>
		</div>
		{/if}



		<div class="detailAmanities">
			<ul class="objectDetailServicesIconList list">

				{var $board = $rental->getAmenitiesByType(array('board'))}
				{if $board}
				   <li class="board">
				   <strong>{_o1010}</strong>
					   <div class="content">
						{foreach $board  as $amenities}					
							{foreach $amenities as $amenity}
								{_$amenity->name}{if !$iterator->last}, {/if}
							{/foreach}					
						{/foreach}
					</div>
				   </li>			   
			   {/if}
			   {var $separateGroups = $rental->getAmenitiesByType(array('separate-groups'))}
			   {* {if $separateGroups} *}
				   <li class="separate-groups">				   
				   <strong>{_o1011}</strong>
					   <div class="content">
						{foreach $separateGroups as $amenities}					
							{foreach $amenities as $amenity}
								{_$amenity->name}{if !$iterator->last}, {/if}
							{/foreach}					
						{/foreach}
						</div>
				   </li>	
			   {* {/if} *}
			   {if $rental->tags}
				   <li class="tag">
				   <strong>{_o1014}</strong>
					   <div class="content">
						{foreach $rental->tags as $tag}					
							{_$tag->name}
						{/foreach}
					</div>
				   </li>
			   {/if}
			   {var $ownerAvailability = $rental->getAmenitiesByType(array('owner-availability'))}
			  {* {if $ownerAvailability} *}
			   <li class="owner-availability">
			   <strong>{_o1012}</strong>
				   <div class="content">
						{foreach $ownerAvailability as $amenities}					
							{foreach $amenities as $amenity}
								{_$amenity->name}{if !$iterator->last}, {/if}
							{/foreach}				
						{/foreach}
				</div>
			   </li>
			   {* {/if} *}
			   {*  {if $rental->rooms} *}
				   <li class="rooms">
				   <strong>{_o23006}</strong>
					   <div class="content">
						{$rental->rooms}
					  </div>
				   </li>
			   {* {/if} *}
			</ul>
		</div>

		<div class="clearfix"></div>



<table class="table table-striped tableDefault autowidth">
					<thead>
						<tr>
							<th>{_o1059}</th>
						</tr>
					</thead>
					<tbody>
					{foreach $rental->getAmenitiesByType(array('important','board','kitchen','bathroom','heating','parking','children','room','relax','service','owner-availability','wellness','congress','other')) as $amenities}
						<tr><td>
						{foreach $amenities as $amenity}
							{if $iterator->first}				
								<b>{_$amenity->type->name}: </b>				
							{/if}

							{_$amenity->name}{if !$iterator->last}, {/if}
						{/foreach}
						</td></tr>
					{/foreach}
					</tbody>
				</table>

	</div>



	<!-- right sidebar -->
	<div class="span3 sidebarRightDetail">

						{if $mainImage = $rental->getMainImage()}
							<button class="marginTop btn btn-compare addToFavorites currentObject" rel="{$rental->id}" link="{link Rental:detail $rental}" thumb="{$basePath}{$mainImage|rentalImageSrc}"><span class="entypo-heart">{_o12334}</span></button>
						{/if}
	

			<script type="text/javascript" src="https://apis.google.com/js/plusone.js"></script>
		<div class="socialIconTable marginBottom marginTop">
        <iframe src="http://www.facebook.com/plugins/like.php?href={link Rental:detail $rental}&amp;locale=sk_SK&amp;layout=button_count&amp;show_faces=false&amp;width=130&amp;action=like&amp;colorscheme=light&amp;height=80" scrolling="no" frameborder="0" style="border:none; overflow:hidden; height:20px; width:130px;" allowTransparency="true"></iframe>
        <g:plusone size="small" href="{link Rental:detail $rental}"  style="margin-top:2px;"></g:plusone>
        </div>
	
		{* <button class="btn btn-compare addToFavorites" rel="{$rental->id}" link="{link Rental:detail $rental}" thumb="{$rental->getMainImage()->getThumbnail()}"><span class="entypo-star">{_o12334}</span></button> *}


		<div class="sidebarContact">
			<div class="sidebarTitle">{_o12275}</div>
			<div class="sidebarInner paddingTop">

				<table>
					<tr>
						<td class="iconcover"><i class="entypo-contact"></i></td>
						<td>	
						<strong>{$rental->contactName}</strong>
						</td>
					</tr>

					<tr>
						<td class="iconcover"><i class="entypo-phone"></i></td>
						<td>		
							
						{foreach $rental->phones as $phone }
							<strong>{$phone->international} {if !$iterator->last}<br/>{/if}</strong>
						{/foreach}
						
						</td>
					</tr>

					<tr>						
						<td class="iconcover"><i class="entypo-mail"></i></td>
						<td>

						
						{foreach $rental->emails as $email }
							<strong><a href="mailto:{$email->value}">#email</a> {if !$iterator->last}<br/>{/if}</strong>
						{/foreach}

						</td>
					</tr>

					<tr>						
						<td class="iconcover"><i class="entypo-language"></i></td>
						<td>
						<strong>{_o1013} </strong><br/> 
						
						
						
						{foreach $rental->spokenLanguages as $language}
							{_$language->name}{if !$iterator->last}, {/if}
						{/foreach}
							

						</td>
					</tr>	

				</table>

				
				<div  id="moreInfoContactTable" class="hide">
				<table>

					<tr>
						<td class="iconcover"><i class="entypo-www"></i></td>
						<td>
						{foreach $rental->urls as $url }
							<strong><a target="_blank" href="{$url->value}">{_o1666}</a><strong> {if !$iterator->last}<br/>{/if}
						{/foreach}						
						</td>
					</tr>

					<tr>						
						<td class="iconcover"><i class="entypo-adress"></i></td>
						<td>
						<strong>{_o2431} </strong><br/> 
						
						
							{$rental->contactName} <br/> {$rental->address->address} <br/> {$rental->address->postalCode} {_$rental->address->locality->name} 
							{if $rental->address->subLocality}
							({$rental->address->subLocality} )
							{/if}
								

						</td>
					</tr>

					<tr>						
						<td class="iconcover"><i class="entypo-location"></i></td>
						<td>
						<strong>#gps:</strong>
						
						<a class="mapsImg">
						
						{$rental->address->getGps()}
						
							</a>	

						</td>
					</tr>
				</table>
				</div>
				<table>
					<tr>						
						<td class="iconcover"><i class="entypo-open"></i></td>
						<td>
						<a href="#" class="toogle" opened="#less option" close="{_o1701}" for="moreInfoContactTable">{_o1701}</a>
						</td>
					</tr>
				</table>
				{include '../../forms/Rental/reservationForm.latte'}

			</div>
		</div>


<script>

$(document).ready(function(){

	$('.datepicker').click(function(){
		$(this).addClass('focus');
	});

	$.datepicker.setDefaults(  $.datepicker.regional[ {$envLanguage->iso} ] );
	$( ".datepicker" ).datepicker({ minDate: 0, maxDate: "+1M +10D" });	
	$( ".datepickerto" ).datepicker({ minDate: 1, maxDate: "+1M +10D" });	
});



			{/cache}
			{include favorites.latte}
			{cache homeSearchBar, if => isset($isHome) && $useTemplateCache, expire => '+1 day'}
				{control searchBar}
			{/cache}
			<div class="span9">
				<div class="innerPadding baseRadius marginBottom baseBorder innerPaddingRitgh overflow">
					<div id="content" class="nopadding">

						{include #content}
					</div>
				</div>
			</div>
			{*{control footer}*}

{control header}
<body>
	<div class="mainHolder">
		<div class="flashMessage"><div n:foreach="$flashes as $flash" class="msg {$flash->type}">{$flash->message}</div></div>
		<div class="mainBody">
			<div class="mainRight">
				{include #content}
			</div>
			<div class="mainLeft">
				{include search.latte}
			</div>
		</div>
		<div class="mainHeader" style="background-image: url({$staticPath}images/sk.country.png);">
			{include header.latte}
		</div>
	</div>
</body>
</html>
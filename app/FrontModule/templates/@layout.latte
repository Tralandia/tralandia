{control head:begin}
	{control css}
	{control js}
	{include og.latte}
{control head:end}
<body data-locale-code="{$localeCode}">
	<div class="container baseRadius" id="container">
		<div class="row">


			{cache $homeCacheId, if => $useTemplateCache, expire => '+1 day'}
			{control header}
			{/cache}
			{* {include favorites.latte} *}
			{include navbar.latte}
			{control flashes}

			{cache homeSearchBar, if => isset($isHome) && $useTemplateCache, expire => '+1 day'}
				{if $showSearchBar}
					{control searchBar}
				{elseif $isWorld}
					{control rootCountries}
				{/if}
			{/cache}
			<div class="span9">
				<div class="innerPadding baseRadius marginBottom baseBorder innerPaddingRitgh overflow noTopMargin">
					<div id="content" class="noPadding">

						{include #content}
					</div>
				</div>
			</div>
			{*{control footer}*}
			{if isset($isHome)}
				{cache $footerCountriesCacheId, if => $useTemplateCache, expire => '+1 day'}
					{*{control countriesFooter}*}
				{/cache}
			{/if}
		</div>
	</div>

	{include smallFooter.latte}
</body>

</html>

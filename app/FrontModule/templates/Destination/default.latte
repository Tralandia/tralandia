{#content}
{cache $allDestinationsCacheOptions['key'],
	if => $allDestinationsCacheOptions['enabled'],
	expire => $allDestinationsCacheOptions['expiration'],
	tags => $allDestinationsCacheOptions['tags']}


	<h1>
	{_o100185}
	{* <span class="counter">({_o100002, $locationRentalsCount ,null,[count=>$locationRentalsCount]})</span> *}
</h1>

<div class=" mytabUi ui-tabs" data-cache="{$rand}">
	<div id="tabs-1" class="ui-tabs-panel">
		<div class="row-fluid">
			{var $links = $getLinks()}
			<div n:foreach="$links as $link" class="span4">
				<ul n:inner-foreach="$link as $row" class="hp-list arrows">
					<li class="arrow">
						<a n:href="RentalList:, \Routers\FrontRoute::LOCATION => $row->entity" title="{_$row['entity']->name}"><span class="hoverUnderline">{_$row['entity']->name}</span> <span>({$row['count']})</span></a>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>


{/cache}
{/#}
